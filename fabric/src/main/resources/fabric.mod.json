{
  "schemaVersion": 1,
  "id": "mob_armor_trims",
  "version": "${version}",
  "name": "Mob Armor Trims",
  "description": "${mod_description}",
  "authors": [
    "majo24"
  ],
  "contact": {
    "homepage": "${github_link}",
    "sources": "${github_link}",
    "issues": "${issues_link}"
  },
  "license": "MIT",
  "icon": "assets/mob_armor_trims/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.mob_armor_trims.majo24.fabric.MobArmorTrimsFabric"
    ],
    "client": [
      "net.mob_armor_trims.majo24.fabric.client.MobArmorTrimsFabricClient"
    ],
    "modmenu": [
      "net.mob_armor_trims.majo24.fabric.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "mob_armor_trims.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*"
  },
  "recommends": {
    "modmenu": ">=${modmenu_version}",
    "cloth-config": ">=${cloth_config_version}"
  },
  "custom": {
		"modmenu": {
			"update_checker": true
		}
	}
}
