{
  "schemaVersion": 1,
  "id": "mob_armor_trims",
  "version": "${version}",
  "name": "Mob Armor Trims",
  "description": "Makes mobs be able to spawn with naturally trimmed armor",
  "authors": [
    "majo24"
  ],
  "contact": {
